{
    "presets":["react", "es2015", "stage-0"],
    "plugins": [
        [
            "babel-plugin-root-import", 
            [
                {
                "rootPathPrefix": "~", 
                "rootPathSuffix": "src"
                },

                {
                "rootPathPrefix": "@",
                "rootPathSuffix": "src/components"
                },

                {
                "rootPathPrefix": "&",
                "rootPathSuffix": "src/containers"
                },
                
                {
                "rootPathPrefix": "$",
                "rootPathSuffix": "src/reducers"
                }

                ]
            ]
        ]
}